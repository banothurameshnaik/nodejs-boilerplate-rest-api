{
  "name": "nodejs-boilerplate-rest-api",
  "version": "1.0.0",
  "description": "Nodejs Boiler plate App REST APIs",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/",
    "test": "jest --passWithNoTests --roots=test --runInBand",
    "coverage": "jest --passWithNoTests --roots=test --coverage --detectOpenHandles --runInBand",
    "serve": "npm run build && node dist/index.js",
    "docker-serve": "node dist/index.js",
    "start": "nodemon src/index.js --exec babel-node",
    "build": "babel src -d dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/banothurameshnaik/nodejs-boilerplate-rest-api.git"
  },
  "keywords": [
    "Nodejs",
    "Boiler",
    "Plate"
  ],
  "author": "Banothu Ramesh Naik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/banothurameshnaik/nodejs-boilerplate-rest-api/issues"
  },
  "homepage": "https://github.com/banothurameshnaik/nodejs-boilerplate-rest-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.11",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.3",
    "nodemon": "^2.0.12"
  }
}
