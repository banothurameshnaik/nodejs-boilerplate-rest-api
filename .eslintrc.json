{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "no-console": 1,
    "import/prefer-default-export": "off",
    "max-len": ["error", 250],
    "no-underscore-dangle": "off"
  }
}
